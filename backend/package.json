    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "recursive-copy": "^2.0.11",
    "@types/multer": "^1.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
